<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.OracleDialect</property>
    <property name="hibernate.connection.driver_class">oracle.jdbc.OracleDriver</property>
    <property name="hibernate.connection.url">jdbc:oracle:thin:@cedinho:1521:orcl</property>
    <property name="hibernate.connection.username">genarus</property>
    <property name="hibernate.connection.password">genarus</property>
    <mapping resource="gym/modelo/pojos/Movimiento.hbm.xml"/>
    <mapping resource="gym/modelo/pojos/Servicio.hbm.xml"/>
    <mapping resource="gym/modelo/pojos/CategoriaContrato.hbm.xml"/>
    <mapping resource="gym/modelo/pojos/ContratoCliente.hbm.xml"/>
    <mapping resource="gym/modelo/pojos/AsistenciaCliente.hbm.xml"/>
    <mapping resource="gym/modelo/pojos/Usuario.hbm.xml"/>
    <mapping resource="gym/modelo/pojos/TipoContrato.hbm.xml"/>
    <mapping resource="gym/modelo/pojos/Persona.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
